<!DOCTYPE html>
<html>
<head>
  <title>Integrator</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render 'shared/navigation' %>

<%= render 'shared/flash' %>

<div id='page'>
	
	<h1>
	<%= @event.name %>
	</h1>

	<%= link_to 'Event page', event_path(@event) %>
	<%= link_to 'Timelines', event_timelines_path(@event) %>

	<div id='dashboard'>
		<%= yield %>
	</div>
	
</div>

</body>
</html>
